      // "foreground": "#7d7d7dca",
      // "fontStyle": ""
      // }
      // },
      // {
      // "scope":"markup.quote",
      // "settings": {
      // "foreground": "#FFFF",
      // "fontStyle": ""
      // }
      // }
    ]
  }
}
