{
  "editor.wordWrap": "wordWrapColumn",
  "workbench.colorTheme": "Gray Matter Light",
  "editor.fontSize": 16,
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn"
  },
  "editor.bracketPairColorization.enabled": false,
  "editor.tokenColorCustomizations": {
    "comments": "#dfdfdf",
    "textMateRules": [
      {
        "scope": "meta.embedded.block.katex markup.inserted.math.display.markdown markup.quote.markdown text.html.markdown foregroundmarkup.inserted",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": "markup.heading.markdown",
        "settings": {
          "foreground": "#cb2b12",
          "fontStyle": "bold"
        }
      }
    ]
  }
}
