{
  "editor.wordWrap": "wordWrapColumn",
  "workbench.colorTheme": "Atom One Dark",
  "editor.fontSize": 13,
  "markdown.styles": ["Style.css"],
  "files.associations": {
    ".mdx":"markdown"
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn"
  },
  "editor.bracketPairColorization.enabled": false,
  "editor.tokenColorCustomizations": {
    "comments": "#dfdfdf",
    "textMateRules": [
      {
        "scope": "meta.embedded.block.katex markup.inserted.math.display.markdown markup.quote.markdown text.html.markdown foregroundmarkup.inserted",
        "settings": {
          "fontStyle": "",
          "foreground": "#58eeff"
        }
      },
      {
        "scope": "markup.fenced_code, markup.inline.raw.string",
        "settings": {
          "foreground": "#ebbf3b",
          "fontStyle": ""
        }
      },
      {
        "scope": "markup.quote.markdown",
        "settings": {
          "foreground": "#ebbf3b",
          "fontStyle": ""
        }
      },
      {
        "scope": "markup.italic, punctuation.definition.italic",
        "settings": {
          "foreground": "#aaaaaa",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.section.markdown, punctuation.definition.heading.markdown",
        "settings": {
          "foreground": "#68b2c1",
          "fontStyle": "bold",
        }
      }
    ]
  }
}
